{
	"name" : "jschaedl/iban",
	"license" : "MIT",
	"type" : "library",
	"description" : "A small library for validating and generating International BankAccount Numbers (IBAN).",
	"keywords" : [
		"iban",
		"validation",
		"generation"
	],
	"homepage" : "http://github.com/jschaedl/Iban",
	"authors" : [{
		"name" : "Jan Schaedlich",
		"email" : "mail@janschaedlich.com",
		"homepage" : "https://www.linkedin.com/in/janschaedlich"
	}],
	"autoload" : {
		"psr-0" : {
			"IBAN" : [
				"library",
				"tests/library"
			]
		}
	},
	"require" : {
		"php" : ">=5.3.9",
		"jschaedl/bav" : "*"
	},
	"require-dev" : {
		"phpunit/phpunit" : "3.7.*",
		"phpunit/php-code-coverage" : ">=1.2.10,<1.3.0",
		"phploc/phploc" : "*",
		"sebastian/phpcpd" : "*"
	},
	"suggest": {
       	"ext-bcmath" : "This library makes use of bcmod function, so an installed bcmath extension is recommended."
	}
}
